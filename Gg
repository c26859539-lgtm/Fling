local s = [[
--// GUI CASTX150 Fly Script by ChatGPT

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "CASTX150_GUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 250, 0, 150)
Frame.Position = UDim2.new(0.05, 0, 0.05, 0)
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Text = "CASTX150"
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 20
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundTransparency = 1
Title.Position = UDim2.new(0.05, 0, 0.05, 0)
Title.Size = UDim2.new(0.9, 0, 0.2, 0)
Title.Parent = Frame

local HideButton = Instance.new("TextButton")
HideButton.Text = "Hide GUI"
HideButton.Size = UDim2.new(0.4, 0, 0.2, 0)
HideButton.Position = UDim2.new(0.55, 0, 0.75, 0)
HideButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
HideButton.TextColor3 = Color3.fromRGB(255, 255, 255)
HideButton.Parent = Frame

local FlyButton = Instance.new("TextButton")
FlyButton.Text = "Fly: OFF"
FlyButton.Size = UDim2.new(0.4, 0, 0.2, 0)
FlyButton.Position = UDim2.new(0.05, 0, 0.75, 0)
FlyButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
FlyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FlyButton.Parent = Frame

local SpeedSlider = Instance.new("TextBox")
SpeedSlider.Text = "Speed: 50"
SpeedSlider.Size = UDim2.new(0.9, 0, 0.2, 0)
SpeedSlider.Position = UDim2.new(0.05, 0, 0.45, 0)
SpeedSlider.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
SpeedSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
SpeedSlider.Parent = Frame

HideButton.MouseButton1Click:Connect(function()
	Frame.Visible = false
	local OpenButton = Instance.new("TextButton")
	OpenButton.Text = "Show CASTX150"
	OpenButton.Size = UDim2.new(0, 150, 0, 40)
	OpenButton.Position = UDim2.new(0.05, 0, 0.05, 0)
	OpenButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	OpenButton.TextColor3 = Color3.fromRGB(255, 255, 255)
	OpenButton.Parent = ScreenGui

	OpenButton.MouseButton1Click:Connect(function()
		Frame.Visible = true
		OpenButton:Destroy()
	end)
end)

local flying = false
local speed = 50
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = char:WaitForChild("HumanoidRootPart")

FlyButton.MouseButton1Click:Connect(function()
	flying = not flying
	FlyButton.Text = "Fly: " .. (flying and "ON" or "OFF")

	if flying then
		local bodyGyro = Instance.new("BodyGyro", humanoidRootPart)
		local bodyVelocity = Instance.new("BodyVelocity", humanoidRootPart)
		bodyGyro.P = 9e4
		bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
		bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)

		while flying and task.wait() do
			local moveDir = Vector3.zero
			local humanoid = char:FindFirstChildOfClass("Humanoid")
			if humanoid then moveDir = humanoid.MoveDirection end
			bodyVelocity.Velocity = moveDir * speed
			bodyGyro.CFrame = workspace.CurrentCamera.CFrame
		end

		bodyGyro:Destroy()
		bodyVelocity:Destroy()
	end
end)

SpeedSlider.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		local value = tonumber(SpeedSlider.Text:match("%d+"))
		if value then
			speed = value
			SpeedSlider.Text = "Speed: " .. value
		else
			SpeedSlider.Text = "Speed: " .. speed
		end
	end
end)
]]

loadstring(s)()
